[
  {
    "job": "InfantSNSummary",
    "active": true,
    "schedule": "every(60).minutes",
    "transients": {
      "select": {
        "modified": {
          "after": {
            "use": "$timeLastRun",
            "event": "InfantSNSummary"
          }
        },
        "channels": {
          "anyOf": [
            "WEIZMANN_INFANTSN",
            "WEIZMANN_INFANTSN_SINGLE"
          ]
